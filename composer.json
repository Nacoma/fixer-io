{
    "name": "nacoma/fixer-io",
    "description": "A wrapper around the fixer.io currency API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Cody Mann",
            "email": "nathancodymann@gmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "psr/http-message": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-client": "^1.0",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "vimeo/psalm": "^4.7",
        "guzzlehttp/guzzle": "^7.3",
        "nyholm/psr7": "^1.4",
        "symfony/var-dumper": "^5.3",
        "cache/array-adapter": "^1.1",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Nacoma\\Fixer\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
